    "markdown-it-footnote": "^3.0.1",
    "markdown-it-ins": "^2.0.0",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.1.0",
    "ora": "^5.1.0",
    "path": "^0.12.7",
    "prettier": "^2.4.1",
    "raw-loader": "^0.5.1",
    "reset-css": "^5.0.1",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "simple-assign": "^0.1.0",
    "stream-browserify": "^3.0.0",
