{
  "name": "@makesenseorg/design-system",
  "version": "1.16.7",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "serve": "http-server ./docs -o -s",
    "build": "npm run theo && vue-cli-service build; mkdir docs/assets; cp -r src/styleguide/assets public/",
    "lint": "vue-cli-service lint",
    "dev": "npm-run-all --parallel theo:onchange theo servedev",
    "install-playground": "cd src/playground && npm install",
    "playground": "npm run build:system && cd src/playground && npm run dev",
    "servedev": "vue-cli-service serve --open",
    "build:components": "cd src && node build-test.js && cd .. && create-index ./dist/components -x js vue -i",
    "build:system": "npm run theo && cross-env BUILD=library NODE_ENV=production vue-cli-service build --target lib --name system ./src/library.js && npm run build:components",
    "build-report:system": "npm run theo && cross-env BUILD=library NODE_ENV=production vue-cli-service build --report --target lib --name system ./src/library.js && npm run build:components",
    "theo": "node ./buildtokens.js",
    "theo:onchange": "onchange \"./src/system/tokens/*.yml\" \"./src/system/tokens/themes/**/*.yml\" -- npm run theo",
    "test:unit": "vue-cli-service test:unit",
    "prepack": "npm run build:system"
  },
  "dependencies": {
    "dayjs": "^1.8.26",
    "eval": "^0.1.4",
    "feather-icons": "^4.28.0",
    "quill-mention": "^3.0.4",
    "reset-css": "^5.0.1",
    "vue": "^2.5.17",
    "vue-feather": "^1.0.1",
    "vue2-editor": "^2.10.2",
    "vue2-google-maps": "^0.10.7"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/standalone": "^7.0.0-beta.56",
    "@vue/cli-plugin-babel": "^3.0.0-rc.12",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.12",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.0-rc.12",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "async-validator": "^1.8.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-plugin-transform-require-context": "^0.0.3",
    "babel-polyfill": "^6.26.0",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.2",
    "child_process": "^1.0.2",
    "clipboard-copy": "^2.0.1",
    "clone-deep": "^4.0.0",
    "codemirror": "^5.39.2",
    "create-index": "^2.6.0",
    "cross-env": "^5.2.0",
    "dot-prop": "^4.2.0",
    "fs": "0.0.1-security",
    "glob": "^7.1.6",
    "husky": "^4.3.0",
    "lodash": "^4.17.20",
    "markdown-it": "^8.4.2",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-ins": "^2.0.0",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.1.0",
    "ora": "^5.1.0",
    "path": "^0.12.7",
    "raw-loader": "^0.5.1",
    "sass": "^1.57.1",
    "sass-loader": "^7.1.0",
    "theo": "^8.1.5",
    "vue-click-outside": "^1.0.7",
    "vue-docgen-api": "^2.3.13",
    "vue-router": "^3.0.1",
    "vue-svg-loader": "^0.8.0",
    "vue-template-compiler": "^2.5.17",
    "vuep": "https://github.com/visualjerk/vuep.git#fix-iframe-firefox",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-merge-and-include-globally": "^2.0.11"
  },
  "author": "makesense (makesense.org)",
  "main": "./dist/system.umd.min.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/makesenseorg/makesense_design-system.git"
  },
  "homepage": "https://makesense-design-system.netlify.com",
  "sideEffects": [
    "*.scss",
    "*.css",
    "**.vue"
  ]
}
