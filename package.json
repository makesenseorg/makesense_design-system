{
  "name": "@makesenseorg/design-system",
  "version": "1.17.12-next.1",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "serve": "http-server ./docs -o -s",
    "build": "npm run build:vue3 && rm -rf docs && cd src/styleguide-vue3 && npm run build && mkdir ../../docs && mv dist/* ../../docs && cd ../../public && cp -r assets 404.html favicon.* brand.svg ../docs/",
    "lint": "vue-cli-service lint",
    "dev": "npm-run-all --parallel theo:onchange theo servedev",
    "install-playground": "cd src/playground && npm install",
    "playground": "npm run build:system && cd src/playground && npm run dev",
    "servedev": "vue-cli-service serve --open",
    "build:components": "cd src && node build-test.js && cd .. && create-index ./dist/components -x js vue -i",
    "build:system": "npm run theo && cross-env BUILD=library NODE_ENV=production vue-cli-service build --target lib --name system ./src/library.js && npm run build:components",
    "build-report:system": "npm run theo && cross-env BUILD=library NODE_ENV=production vue-cli-service build --report --target lib --name system ./src/library.js && npm run build:components",
    "theo": "node ./buildtokens.js",
    "theo:onchange": "onchange \"./src/system/tokens/*.yml\" \"./src/system/tokens/themes/**/*.yml\" -- npm run theo",
    "test:unit": "vue-cli-service test:unit",
    "prepack": "npm run theo && npm run build:vue3",
    "build:vue3": "cd src && node build-v3.js",
    "serve:vue3": "onchange \"./src/system/**/*.js\" \"./src/system/**/*.vue\" -- npm run build:vue3",
    "dev:guide": "cd src/styleguide-vue3 && npm run serve",
    "serve:guide": "npm-run-all --parallel build:vue3 theo:onchange serve:vue3 dev:guide"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "core-js": "^3.8.3",
    "dayjs": "^1.8.26",
    "feather-icons": "^4.28.0",
    "quill-mention": "^3.0.4",
    "vue": "^3.2.47",
    "vue-datepicker-next": "^1.0.2",
    "vue3-quill": "^0.3.0",
    "vue3-runtime-template": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/standalone": "^7.0.0-beta.56",
    "@vitejs/plugin-vue": "^1.6.1",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "async-validator": "^1.8.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-plugin-transform-require-context": "^0.0.3",
    "babel-polyfill": "^6.26.0",
    "buffer": "^6.0.3",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.2",
    "child_process": "^1.0.2",
    "clipboard-copy": "^2.0.1",
    "clone-deep": "^4.0.0",
    "codemirror": "^5.39.2",
    "create-index": "^2.6.0",
    "cross-env": "^5.2.0",
    "dot-prop": "^4.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "fs": "0.0.1-security",
    "glob": "^7.1.6",
    "husky": "^4.3.0",
